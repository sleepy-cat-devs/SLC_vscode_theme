	"colors": {
		// エディター設定
		"editor.background": "#000e14",
		"editor.foreground": "#dbf1fe",
		"editorBracketHighlight.foreground1": "#dbf1fecd",
		"editorBracketHighlight.foreground2": "#69cf5ecd",
